//判断一个数是否为素数
#include<stdio.h>
int main(){
	int a;
	int b;
	scanf("%d",&b); //接收用户的输入
	for(a = 2;a < b;a++){
		if(b % a == 0){ //判断这个数能否被除了1和它本身之外的数整除
			printf("NO\n"); //如果能则不是素数
			break;
		}
	}
	if(b == a){ //如果这个数只能被1和它本身整除
		printf("YES\n"); //则是素数
	}
	if(b == 1){  //1不是素数
		printf("NO\n");
	}
	return 0;
}
